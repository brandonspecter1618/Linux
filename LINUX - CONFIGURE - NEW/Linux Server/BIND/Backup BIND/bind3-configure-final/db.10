;
; BIND zone file for 10.61.10.xxx
;

$TTL    3H
@       IN      SOA     networksystem2.com               root.networksystem2.com. (
                        2017061201      ; serial
                        8H              ; refresh
                        2H              ; retry
                        4W              ; expire
                        1D )            ; minimum
;
@               NS      ns.networksystem2.com.    ; Nameserver address

100             PTR     server.networksystem2.com.
100             PTR     ns.networksystem2.com.
100             PTR     mail.networksystem2.com.
101             PTR     virtual.networksystem2.com.
1               PTR     router.networksystem2.com.
102             PTR     networksecurity2.com
